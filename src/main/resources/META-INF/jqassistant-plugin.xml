<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.10"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.10 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.10.xsd"
                    name="jQAssistant RDBMS Plugin" id="jqa.plugin.rdbms">
    <description>Provides a scanner for importing the metadata of a relational database schema.</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.RdbmsDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ConnectionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.SchemaDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.TableDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ViewDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ColumnDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ColumnTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.SequenceDesriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.IndexDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.IndexOnColumnDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.PrimaryKeyDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.PrimaryKeyOnColumnDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ForeignKeyDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ForeignKeyReferenceDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.TriggerDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.RoutineDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.FunctionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.ProcedureDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.model.RoutineColumnDescriptor</class>
    </model>
    <scope>
        <class>com.buschmais.jqassistant.plugin.rdbms.api.RdbmsScope</class>
    </scope>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.rdbms.impl.scanner.ConnectionPropertyFileScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.rdbms.impl.scanner.ConnectionUriScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
